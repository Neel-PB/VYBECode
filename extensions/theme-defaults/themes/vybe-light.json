{
	"$schema": "vscode://schemas/color-theme",
	"name": "VYBE Light",
	"type": "light",
	"colors": {
		"activityBar.activeBorder": "#3ecf8e",
		"activityBar.background": "#eceff2",
		"activityBar.border": "#eceff2",
		"activityBar.foreground": "#1F1F1F",
		"activityBar.inactiveForeground": "#616161",
		"activityBarBadge.background": "#3ecf8e",
		"activityBarBadge.foreground": "#FFFFFF",
		"badge.background": "#3ecf8e",
		"badge.foreground": "#FFFFFF",
		"button.background": "#3ecf8e",
		"button.border": "#0000001a",
		"button.foreground": "#FFFFFF",
		"button.hoverBackground": "#35b87d",
		"button.secondaryBackground": "#eceff2",
		"button.secondaryForeground": "#3B3B3B",
		"button.secondaryHoverBackground": "#d8dde2",
		"checkbox.background": "#ffffff",
		"checkbox.border": "#CECECE",
		"descriptionForeground": "#616161",
		"dropdown.background": "#FFFFFF",
		"dropdown.border": "#CECECE",
		"dropdown.foreground": "#3B3B3B",
		"dropdown.listBackground": "#FFFFFF",
		"editor.background": "#FFFFFF",
		"editor.foreground": "#3B3B3B",
		"editor.selectionBackground": "#3ecf8e40",
		"editor.selectionHighlightBackground": "#3ecf8e20",
		"editorGroup.border": "#eceff2",
		"editorGroupHeader.tabsBackground": "#f8f8f9",
		"editorGroupHeader.tabsBorder": "#eceff2",
		"editorLineNumber.activeForeground": "#3ecf8e",
		"editorLineNumber.foreground": "#6E7681",
		"editorWidget.background": "#ffffff",
		"errorForeground": "#F85149",
		"focusBorder": "#3ecf8e",
		"foreground": "#3B3B3B",
		"icon.foreground": "#3B3B3B",
		"input.background": "#FFFFFF",
		"input.border": "#CECECE",
		"input.foreground": "#3B3B3B",
		"input.placeholderForeground": "#767676",
		"inputOption.activeBackground": "#3ecf8e20",
		"inputOption.activeBorder": "#3ecf8e",
		"inputOption.activeForeground": "#000000",
		"list.activeSelectionBackground": "#3ecf8e20",
		"list.activeSelectionForeground": "#000000",
		"list.hoverBackground": "#F2F2F2",
		"list.focusAndSelectionOutline": "#3ecf8e",
		"menu.selectionBackground": "#3ecf8e",
		"menu.selectionForeground": "#ffffff",
		"panel.background": "#FFFFFF",
		"panel.border": "#eceff2",
		"panelTitle.activeBorder": "#3ecf8e",
		"panelTitle.activeForeground": "#3B3B3B",
		"panelTitle.inactiveForeground": "#616161",
		"progressBar.background": "#3ecf8e",
		"quickInput.background": "#ffffff",
		"quickInput.foreground": "#3B3B3B",
		"sideBar.background": "#FFFFFF",
		"sideBar.border": "#eceff2",
		"sideBar.foreground": "#3B3B3B",
		"sideBarSectionHeader.background": "#FFFFFF",
		"sideBarSectionHeader.border": "#eceff2",
		"sideBarSectionHeader.foreground": "#3B3B3B",
		"sideBarTitle.foreground": "#3B3B3B",
		"statusBar.background": "#eceff2",
		"statusBar.border": "#eceff2",
		"statusBar.debuggingBackground": "#3ecf8e",
		"statusBar.debuggingForeground": "#FFFFFF",
		"statusBar.foreground": "#3B3B3B",
		"statusBar.noFolderBackground": "#eceff2",
		"statusBarItem.prominentBackground": "#3ecf8e20",
		"statusBarItem.remoteBackground": "#3ecf8e",
		"statusBarItem.remoteForeground": "#FFFFFF",
		"tab.activeBackground": "#FFFFFF",
		"tab.activeBorder": "#3ecf8e",
		"tab.activeForeground": "#3B3B3B",
		"tab.border": "#eceff2",
		"tab.inactiveBackground": "#f8f8f9",
		"tab.inactiveForeground": "#616161",
		"terminal.ansiGreen": "#3ecf8e",
		"textLink.activeForeground": "#35b87d",
		"textLink.foreground": "#3ecf8e",
		"titleBar.activeBackground": "#eceff2",
		"titleBar.activeForeground": "#3B3B3B",
		"titleBar.border": "#eceff2",
		"titleBar.inactiveBackground": "#eceff2",
		"titleBar.inactiveForeground": "#616161",
		"walkthrough.stepTitle.foreground": "#3B3B3B"
	},
	"tokenColors": [
		{
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#6A737D",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#D73A49"
			}
		},
		{
			"scope": ["string", "string.quoted", "string.template"],
			"settings": {
				"foreground": "#032F62"
			}
		},
		{
			"scope": ["constant.numeric", "constant.language"],
			"settings": {
				"foreground": "#005CC5"
			}
		},
		{
			"scope": ["entity.name.function", "support.function"],
			"settings": {
				"foreground": "#6F42C1"
			}
		},
		{
			"scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
			"settings": {
				"foreground": "#005CC5"
			}
		},
		{
			"scope": ["variable", "variable.other", "variable.parameter"],
			"settings": {
				"foreground": "#24292E"
			}
		},
		{
			"scope": ["variable.language"],
			"settings": {
				"foreground": "#005CC5",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["constant.character.escape"],
			"settings": {
				"foreground": "#005CC5"
			}
		},
		{
			"scope": ["keyword.operator"],
			"settings": {
				"foreground": "#D73A49"
			}
		},
		{
			"scope": ["punctuation"],
			"settings": {
				"foreground": "#24292E"
			}
		},
		{
			"scope": [
				"meta.property-name",
				"support.type.property-name",
				"variable.other.property",
				"variable.other.object.property",
				"meta.object-literal.key",
				"meta.object.member",
				"entity.name.tag.yaml",
				"support.type.property-name.css",
				"support.type.property-name.scss",
				"support.type.property-name.less",
				"support.type.property-name.sass",
				"meta.property-name.json",
				"meta.property-name.json5",
				"support.type.property-name.json",
				"meta.attribute-name",
				"meta.attribute"
			],
			"settings": {
				"foreground": "#005CC5"
			}
		},
		{
			"scope": ["entity.name.tag"],
			"settings": {
				"foreground": "#22863A"
			}
		},
		{
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#6F42C1"
			}
		},
		{
			"scope": ["markup.heading"],
			"settings": {
				"foreground": "#005CC5",
				"fontStyle": "bold"
			}
		},
		{
			"scope": ["markup.bold"],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#D73A49"
			}
		},
		{
			"scope": ["markup.italic"],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["markup.inline.raw"],
			"settings": {
				"foreground": "#005CC5"
			}
		}
	]
}

